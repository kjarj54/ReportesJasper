	<queryString language="SQL">
		<![CDATA[SELECT 
  u.usu_id, 
  u.usu_nombre, 
  u.usu_papellido, 
  u.usu_correo,
  u.usu_cedula,
  usu_tipousuario,
  usu_usuario,
  usu_idioma,
  usu_activo,
  CASE 
    WHEN CLI_MEDICO.usu_id IS NULL OR CLI_MEDICO.usu_id < 1 THEN 'No'
    ELSE 'Si'
  END AS medico_asignado
